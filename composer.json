{
    "name": "mwadon/gusclient",
    "description": "GUS companies database API client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Michał Wójtowicz",
            "email": "poczta@mwojtowicz.it"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "php-coveralls/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4" : {
            "MWojtowicz\\GusClient\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "Tests\\MWojtowicz\\GusClient\\" : "tests/"
        }
    },
    "archive": {
        "exclude": [
            "tests/",
            ".gitignore",
            ".travis.yml",
            "phpunit.xml.dist",
            "build/",
            "composer.lock"
        ]
    }
}
